    android:height="200dp"
    android:viewportWidth="200"
    android:viewportHeight="200">
    
    <!-- Capybara body -->
    <path
        android:fillColor="#D2691E"
        android:pathData="M50,120 Q50,90 80,90 L120,90 Q150,90 150,120 L150,160 Q150,180 130,180 L70,180 Q50,180 50,160 Z"/>
    
    <!-- Capybara head -->
    <ellipse
        android:fillColor="#D2691E"
        android:cx="100"
        android:cy="80"
        android:rx="45"
        android:ry="35"/>
    
    <!-- Eyes -->
    <circle
        android:fillColor="#000000"
        android:cx="85"
        android:cy="75"
        android:r="4"/>
