        android:rx="3"
        android:ry="2"/>
    
    <!-- Mouth -->
    <path
        android:strokeColor="#8B4513"
        android:strokeWidth="2"
        android:fillColor="none"
        android:pathData="M95,90 Q100,95 105,90"/>
    
    <!-- Ears -->
    <ellipse
