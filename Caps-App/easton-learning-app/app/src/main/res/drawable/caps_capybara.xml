    <ellipse
        android:fillColor="#D2691E"
        android:cx="70"
        android:cy="175"
        android:rx="8"
        android:ry="15"/>
    <ellipse
        android:fillColor="#D2691E"
        android:cx="90"
        android:cy="175"
        android:rx="8"
        android:ry="15"/>
    <ellipse
        android:fillColor="#D2691E"
        android:cx="110"
        android:cy="175"
        android:rx="8"
        android:ry="15"/>
    <ellipse
        android:fillColor="#D2691E"
        android:cx="130"
        android:cy="175"
        android:rx="8"
        android:ry="15"/>
